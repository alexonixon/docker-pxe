default vesamenu.c32
prompt 1
timeout 600

label linux
menu label ^Install Fedora Workstation 23 64-bit
menu default
kernel f23/vmlinuz
append initrd=f23/initrd.img inst.stage2=http://download.fedoraproject.org/pub/fedora/linux/releases/23/Workstation/x86_64/os/ ip=dhcp

label local
menu label Boot from ^local drive
localboot 0xffff
